{
    "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
    "type": "datasource",
    "name": "PRTG",
    "id": "maxmarkusprogram-prtg-datasource",
    "metrics": true,
    "backend": true,
    "annotations": true,
    "logs": true,
    "tracing": true,
    "streaming": true,
    "executable": "gpx_prtg",
    "alerting": true,
    "info": {
        "description": "Grafana datasource plugin whit backend for PRTG",
        "author": {
            "name": "Mustafa Ã–zdemir",
            "email": "mustafa.ozdemir1408@gmail.com"
        },
        "keywords": [
            "datasource",
            "PRTG",
            "grafana"
        ],
        "logos": {
            "small": "img/logo.svg",
            "large": "img/logo.svg"
        },
        "links": [
            {
                "name": "GitHub",
                "url": "https://github.com/1DeliDolu/PRTG.git"
            }
        ],
        "screenshots": [
            {
                "path": "img/1739797413883.png"
            }
        ],
        "version": "1.0.0",
        "updated": "2025-05-20"
    },
    "routes": [
        {
            "path": "{{.JsonData.url}}",
            "method": "GET",
            "urlParams": [
                {
                    "name": "apitoken",
                    "content": "{{.SecureJsonData.apiKey}}"
                }
            ]
        }
    ],
    "includes": [
        {
            "name": "overview",
            "path": "dashboards/overview.json",
            "type": "dashboard"
        }
    ],
    "dependencies": {
        "grafanaDependency": ">=10.4.0",
        "plugins": [
            {
                "id": "grafana-prtg-datasource",
                "name": "PRTG-GRAFANA",
                "type": "datasource",
                "version": "1.0.0"
            }
        ]
    },
    "buildMode": "production"
}